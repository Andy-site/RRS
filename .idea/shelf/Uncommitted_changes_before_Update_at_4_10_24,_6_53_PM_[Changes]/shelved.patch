<+><!DOCTYPE html>\n<html lang=\"en\">\n<head>\n\n    {% block head%}  {% endblock %}\n          {% load static %}\n    <meta charset=\"UTF-8\">\n\n        <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-alpha1/dist/css/bootstrap.min.css\" rel=\"stylesheet\">\n    <title>{% block title%}  {% endblock %}</title>\n\n            <style>\n\n            body{\nbackground-color: #e9ecef;\n}\n\n.btn-primary {\n    color: #fff;\n    background-color: #28a745;\n    border-color: #28a745;\n}\n.btn-primary:hover {\n    color: #28a745;\n    background-color: #fff;\n    border-color: #fff;\n}\n\n\n.card{\n\nwidth: 400px;\nborder:none;\n\n}\n\n\n\n\n.btr{\n\nborder-top-right-radius: 5px !important;\n}\n\n\n.btl{\n\nborder-top-left-radius: 5px !important;\n}\n\n.btn-dark {\ncolor: #fff;\nbackground-color: #28a745;\nborder-color: #28a745;\n}\n\n\n.btn-dark:hover {\ncolor: #fff;\nbackground-color: #28a745;\nborder-color: #28a745;\n}\n\na {\n    color: #28a745;\n    text-decoration: underline;\n}\na:hover{\n color: #28a745;\n}\n\n.nav-pills{\n\ndisplay:table !important;\nwidth:100%;\n}\n\n.nav-pills .nav-link.active, .nav-pills .show>.nav-link {\n    color: #fff;\n    background-color: #28a745;\n}\n\n.nav-pills .nav-link {\nborder-radius: 0px;\nborder-bottom: 1px solid #0d6efd40;\n\n}\n\n.nav-item {\n    display: table-cell;\n    background: #0dfd462e;\n}\n\n\n.form{\n\npadding: 10px;\nheight: 300px;\n}\n\n.form input{\n\nmargin-bottom: 12px;\nborder-radius: 3px;\n}\n\n\n.form input:focus{\n\nbox-shadow: none;\n}\n\n\n.form button{\n\nmargin-top: 20px;\n}\n\n.connect-link {\n    color: white;\n    cursor: pointer;\n}\n\nelement.style {\n    position: absolute;\n    top: 45px;\n    left: 1367px;\n\n\n}\n.connect-link:hover {\n    color: green;\n}\n#dropdownMenu {\n    text-decoration: none;\n    display: block;\n     margin-bottom: 20px;\n     margin-left:50px;\n     margin-top: 50px;\n  }\n\n#connect-links-container {\n    background-color: rgba(0, 0, 0, 0.1); /* Black with 80% opacity */\n    font-size: 20px;\n    position: absolute;\n    display: flex;\n    top: 50px;\n    left: 430px;\n    width: 620px;\n    border-radius: 10px; /* Adjust the value as needed */\n}\n.me:hover {\n    color: red !important;\n}\n\n\n\n\n\n                {% block css %}\n                 {% endblock %}\n        </style>\n</head>\n<body>\n\n{% block body%}  {% endblock %}\n\n\n<!-- Logout Confirmation Modal -->\n<div class=\"modal fade\" id=\"logoutModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"logoutModalLabel\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"logoutModalLabel\">Confirm Logout</h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;</span>\n        </button>\n      </div>\n      <div class=\"modal-body\">\n        Are you sure you want to logout?\n      </div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Cancel</button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"logout()\">Logout</button>\n      </div>\n    </div>\n  </div>\n</div>\n\n\n\n\n\n\n\n\n<!-- to trigger Login modal -->\n<div id=\"connect-links-container\">\n    <span class=\"connect-link\" style=\"display: block; margin-bottom: 15px; margin-left: 50px; margin-top: 22px; \" onmousedown=\"preventDefaultAndNavigate('{% url 'home' %}');\">Home</span>\n    <span class=\"connect-link\" style=\"display: block;margin-bottom: 15px; margin-left: 50px; margin-top: 22px;\" onmousedown=\"preventDefaultAndNavigate('{% url 'about' %}');\">About Us</span>\n    <span class=\"connect-link\" style=\"display: block; margin-bottom: 15px; margin-left: 50px; margin-top: 22px;\" onmousedown=\"preventDefaultAndNavigate('{% url 'menu' %}');\">Menu</span>\n    <span class=\"connect-link\" style=\"display: block; margin-bottom: 15px; margin-left: 50px; margin-top: 22px;\" onmousedown=\"preventDefaultAndNavigate('{% url 'rev123' %}');\">Reviews</span>\n\n{% if user.is_authenticated %}\n<div class=\"me\" style=\"cursor: pointer; display: block; margin-bottom: 15px; margin-left: 50px; margin-top: 22px; color: white;\" data-toggle=\"modal\" data-target=\"#logoutModal\">\n    {{ request.user }}\n</div>\n{% else %}\n<span class=\"connect-link\" data-toggle=\"modal\" data-target=\"#loginModal\" style=\"display: block;margin-bottom: 5px; margin-left: 50px; margin-top: 22px; \">Connect</span>\n{% endif %}\n\n</div>\n\n\n\n\n\n      {% if messages %}\n      <div class=\"container\">\n         {% for message in messages %}\n         <div class=\"alert {% if message.tags == 'error' %}alert-danger{% elif message.tags == 'success' %}alert-success{% endif %} alert-dismissible fade show\" role=\"alert\">\n          <strong>{{ message }}</strong>\n        <button type=\"button\" class=\"close\" data-dismiss=\"alert\" aria-label=\"Close\">\n            <span aria-hidden=\"true\">&times;</span>\n        </button>\n         </div>\n          {% endfor %}\n          </div>\n      {% endif %}\n\n\n\n<div class=\"modal\" id=\"loginModal\" tabindex=\"-1\" aria-labelledby=\"loginModal\" aria-hidden=\"true\">\n    <div class=\"d-flex justify-content-center align-items-center mt-5\">\n        <div class=\"card\">\n            <div class=\"modal-header\">\n                <h5 class=\"modal-title\">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Connect with us</h5>\n                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                    <span aria-hidden=\"true\">&times;</span>\n                </button>\n            </div>\n            <ul class=\"nav nav-pills mb-3\" id=\"pills-tab\" role=\"tablist\">\n                <li class=\"nav-item text-center\">\n                    <a class=\"nav-link active btl\" id=\"pills-home-tab\" data-toggle=\"pill\" href=\"#pills-home\" role=\"tab\" aria-controls=\"pills-home\" aria-selected=\"true\">Login</a>\n                </li>\n                <li class=\"nav-item text-center\">\n                    <a class=\"nav-link btr\" id=\"pills-profile-tab\" data-toggle=\"pill\" href=\"#pills-profile\" role=\"tab\" aria-controls=\"pills-profile\" aria-selected=\"false\">Signup</a>\n                </li>\n            </ul>\n            <div class=\"tab-content\" id=\"pills-tabContent\">\n                <div class=\"tab-pane fade show active\" id=\"pills-home\" role=\"tabpanel\" aria-labelledby=\"pills-home-tab\">\n                    <div class=\"form px-4 pt-5\">\n                        <form action=\"/handle2/\" method='post'>\n                            <input type=\"text\" name=\"username\" class=\"form-control\" placeholder=\"Username\" autocomplete=\"off\">\n                            <input type=\"password\" name=\"password\" class=\"form-control\" placeholder=\"Password\" autocomplete=\"off\">\n                            {% csrf_token %}\n                            <button class=\"btn btn-dark btn-block\">Login</button>\n                        </form>\n                    </div>\n                </div>\n                <div class=\"tab-pane fade\" id=\"pills-profile\" role=\"tabpanel\" aria-labelledby=\"pills-profile-tab\">\n                    <div class=\"form px-4\">\n                        <form action=\"/handle1/\" method='post'>\n                            <input type=\"text\" name=\"username\" class=\"form-control\" placeholder=\"Name\" autocomplete=\"off\">\n                            <input type=\"text\" name=\"email\" class=\"form-control\" placeholder=\"Email\" autocomplete=\"off\">\n                            <input type=\"text\" name=\"phone\" class=\"form-control\" placeholder=\"Phone\" autocomplete=\"off\">\n                            <input type=\"text\" name=\"password\" class=\"form-control\" placeholder=\"Password\" autocomplete=\"off\">\n                            {% csrf_token %}\n                            <button class=\"btn btn-dark btn-block\">Signup</button>\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n\n\n<script src=\"https://code.jquery.com/jquery-3.5.1.slim.min.js\" ></script>\n<script src=\"https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js\" ></script>\n<script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js\" ></script>\n\n\n\n<script>\nfunction toggleDropdown() {\n  var dropdownMenu = document.getElementById(\"dropdownMenu\");\n  if (dropdownMenu.style.display === \"none\") {\n    dropdownMenu.style.display = \"block\";\n  } else {\n    dropdownMenu.style.display = \"none\";\n  }\n}\n</script>\n\n<script>\nfunction preventDefaultAndNavigate(url) {\n    event.preventDefault();\n    window.location.href = url;\n}\n</script>\n\n<script>\n\nfunction logout() {\n    // Perform logout action, for example, redirect to logout URL\n   window.location.href = \"{% url 'lout' %}\";\n}\n\n</script>\n\n\n\n\n\n\n</body>\n</html>
