<+>{% extends 'myapp/admin_page.html' %}\n\n{% block title %}Menu{% endblock %}\n\n{% block css %}\n.menu123 {\n    position: absolute;\n    font-size: 40px;\n    left: 650px;\n    top: 55px;\n}\n\n.menu {\n    position: absolute;\n    font-size: 20px; /* Adjust font size as needed */\n    left: 500px; /* Adjust left position as needed */\n    top: 185px;\n}\n\n.menu ul {\n    list-style-type: none;\n    padding: 0;\n    margin: 0;\n}\n\n.menu li {\n    display: inline; /* Display menu items inline */\n    margin-right: 20px; /* Adjust margin between items as needed */\n}\n\n.menu a {\n    text-decoration: none;\n    color: black; /* Adjust link color as needed */\n}\n\n.carousel-container {\n    width: 800px; /* Adjust carousel width as needed */\n    overflow: hidden;\n    position: absolute;\n    left: 500px; /* Adjust left position as needed */\n    top: 300px; /* Adjust top position as needed */\n    height: 400px; /* Adjust carousel height as needed */\n}\n\n.carousel {\n    display: flex;\n    flex-direction: column; /* Change to vertical direction */\n    transition: transform 0.5s ease-in-out;\n}\n\n.carousel-item {\n    flex: 0 0 auto;\n    margin-bottom: 20px; /* Adjust margin between items as needed */\n}\n\n.carousel-item table {\n    width: 100%; /* Adjust table width */\n}\n\n.carousel-item img {\n    max-width: 100px; /* Adjust image width */\n}\n\n{% endblock %}\n\n{% block content %}\n<div class=\"menu123\">Menu</div>\n<div class=\"menu\">\n    <ul>\n        <li><a href=\"#\" class=\"menu-link\" data-target=\"starter\">Starter</a></li>\n        <li><a href=\"#\" class=\"menu-link\" data-target=\"main-courses\">Main Courses</a></li>\n        <li><a href=\"#\" class=\"menu-link\" data-target=\"drink\">Drink</a></li>\n        <li><a href=\"#\" class=\"menu-link\" data-target=\"dessert\">Dessert</a></li>\n    </ul>\n</div>\n\n<div class=\"carousel-container\">\n    <div class=\"carousel\">\n        <div class=\"carousel-item\" id=\"starter\">\n        